{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "storageAccountName": {
      "value": "ghtest123456"
    },
    "location": {
      "value": "centralus"
    },
    "kind": {
      "value": "StorageV2"
    },
    "skuName": {
      "value": "Standard_LRS"
    },
    "accessTier": {
      "value": "Hot"
    },
    "defaultToOAuthAuthentication": {
      "value": true
    },
    "allowSharedKeyAccess": {
      "value": false
    },
    "allowBlobPublicAccess": {
      "value": false
    },
    "containerServices": {
      "value": {
        "deleteRetentionPolicy": {
          "enabled": true,
          "days": 7
        },
        "containerDeleteRetentionPolicy": {
          "enabled": true,
          "days": 7
        },
        "containers": [
          {
            "name": "test-container",
            "publicAccess": "None",
            "metadata": {
              "purpose": "testing"
            }
          }
        ]
      }
    },
    "queueServices": {
      "value": {
        "queues": [
          {
            "name": "test-queue"
          }
        ]
      }
    },
    "tableServices": {
      "value": {
        "tables": [
          {
            "name": "testtable"
          }
        ]
      }
    },
    "tags": {
      "value": {
        "Environment": "Test",
        "ManagedBy": "Pester",
        "Purpose": "ModuleTesting"
      }
    }
  }
}
